<%= form_with scope: resource, as: resource_name, url: session_path(resource_name), local: true do |f| %>
<div class="form-group">
  <%= f.email_field :email, :type => "hidden", :value => "test@example.com", autofocus: true, autocomplete: "email" %>
</div>

<div class="form-group">
  <%= f.password_field :password, :type => "hidden", :value => "1111111", autofocus: true, autocomplete: "current-password" %>
</div>

<div class="actions">
  <%= f.submit "簡単ログイン", class: "btn button_h1 w-100" %>
</div>
<% end %>
